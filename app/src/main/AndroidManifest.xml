<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uowee.droid.util">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RootActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".LinearLayoutHelperActivity"></activity>
        <activity android:name=".GridLayoutHelperActivity"></activity>
        <activity android:name=".StaggeredGridLayoutHelperActivity"></activity>
        <activity android:name=".ColumnLayoutHelperActivity"></activity>
        <activity android:name=".SingleLayoutHelperActivity"></activity>
        <activity android:name=".StickyLayoutHelperActivity"></activity>
        <activity android:name=".ScrollFixLayoutHelperActivity"></activity>
        <activity android:name=".FixLayoutHelperActivity"></activity>
        <activity android:name=".FloatLayoutHelperActivity"></activity>
        <activity android:name=".OnePlusNLayoutHelperActivity"></activity>
    </application>

</manifest>